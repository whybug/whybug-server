{
  "name": "wtf-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node_modules/.bin/pm2 start server.js --name whybug -i 0",
    "stop": "node_modules/.bin/pm2 stop whybug",
    "dev": "bash -c '. ./.env && nodemon --ignore assets server.js & npm run asset-server'",
    "asset-server": "bash -c '. ./.env && ./node_modules/.bin/webpack-dev-server --config config/webpack.config.dev.js --host 0.0.0.0 --hot --port 8001'",
    "knex": "node_modules/.bin/knex --env production --cwd ./ --knexfile config/knexfile.js",
    "test": "node_modules/.bin/mocha tests --require tests/es6_mocha.js --recursive"
  },
  "dependencies": {
    "bell": "^1.3.0",
    "bookshelf": "^0.7.9",
    "classnames": "^2.1.1",
    "elasticsearch": "^3.0.0",
    "elasticsearch-hapi-logger": "0.0.1",
    "good": "^4.0.0",
    "good-console": "^2.0.0",
    "good-reporter": "^3.0.1",
    "handlebars": "^2.0.0",
    "hapi": "^7.5.0",
    "hapi-as-promised": "^1.0.1",
    "hapi-auth-cookie": "^1.3.2",
    "hapi-cache-buster": "^0.2.0",
    "joi": "^4.8.0",
    "knex": "^0.6.22",
    "marked": "^0.3.2",
    "mysql": "^2.5.0",
    "node-uuid": "^1.4.1",
    "pm2": "^0.12.14",
    "pmx": "^0.3.11",
    "promise": "^6.1.0",
    "react": "^0.13.3",
    "react-intl": "^1.1.0",
    "react-markdown-textarea": "^0.14.2",
    "react-router": "^0.13.3",
    "slug": "^0.7.1",
    "superagent": "^0.18.2",
    "superagent-promise": "^0.2.0"
  },
  "devDependencies": {
    "babel": "^5.4.2",
    "babel-core": "^5.4.2",
    "babel-loader": "^5.1.0",
    "babel-runtime": "^5.4.2",
    "chai": "^1.9.1",
    "chai-as-promised": "^4.1.1",
    "chai-react": "0.0.4",
    "css-loader": "^0.9.1",
    "extract-text-webpack-plugin": "^0.3.8",
    "mocha": "^2.1.0",
    "node-sass": "^2.0.1",
    "react-hot-loader": "^1.2.7",
