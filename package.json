{
  "name": "whybug-server",
  "engines": {
    "iojs": ">= 2.5"
  },
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "nodemon --watch src server.js & npm run asset-server",
    "prod": "pm2 start server.js --name whybug -i 0",
    "stop": "pm2 stop whybug'",
    "build": "webpack --config config/webpack.config.js -p 2<&1",
    "asset-server": "webpack-dev-server --config config/webpack.config.dev.js --host 0.0.0.0 --hot --port 8001",
    "test": "mocha tests --require tests/es6_mocha.js --recursive"
  },
  "dependencies": {
    "bell": "^2.7.1",
    "bookshelf": "^0.7.9",
    "boom": "^2.7.1",
    "classnames": "^2.1.1",
    "couchdb-bootstrap": "^1.6.0",
    "dotenv": "^1.2.0",
    "elasticsearch": "^9.0.0",
    "elasticsearch-hapi-logger": "0.0.1",
    "express": "^4.13.3",
    "good": "^6.1.2",
    "good-console": "^5.0.0",
    "good-reporter": "^3.0.1",
    "graphql": "^0.2.2",
    "handlebars": "^2.0.0",
    "hapi": "^8.4.0",
    "hapi-auth-cookie": "^2.2.0",
    "hapi-cache-buster": "^0.4.0",
    "hapi-relax": "^1.0.1",
    "joi": "^4.8.0",
    "knex": "^0.6.22",
    "marked": "^0.3.2",
    "mysql": "^2.5.0",
    "node-uuid": "^1.4.1",
    "pm2": "^0.12.14",
    "pmx": "^0.3.11",
    "promise": "^6.1.0",
    "react": "^0.13.3",
    "react-intl": "^1.1.0",
    "react-markdown-textarea": "^0.14.2",
    "react-router": "^0.13.3",
    "slug": "^0.7.1",
    "superagent": "^1.2.0",
    "superagent-promise": "^0.2.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.24",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-react-transform": "^1.0.3",
    "babel-runtime": "^5.8.24",
    "chai": "^1.9.1",
    "chai-as-promised": "^4.1.1",
    "chai-react": "0.0.4",
    "chokidar": "^1.0.6",
    "css-loader": "^0.9.1",
    "extract-text-webpack-plugin": "^0.8.0",
    "mocha": "^2.1.0",
    "node-sass": "^3.4.1",
    "react-test-utils": "0.0.1",
    "react-transform-hmr": "^1.0.0",
    "sass-loader": "^3.1.1",
    "sinon": "^1.10.3",
    "sinon-chai": "^2.5.0",
    "style-loader": "^0.12.2",
    "supertest": "^0.13.0",
    "webpack": "^1.12.1",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.0.1",
    "webpack-isomorphic-tools": "^0.9.3"
  },
  "babel": {
    "stage": 0,
    "optional": [
      "runtime",
      "utility.inlineEnvironmentVariables"
    ],
    "blacklist": [
      "es6.properties.shorthand",
      "es6.classes",
      "es6.forOf",
      "es6.constants",
      "es6.templateLiterals"
    ]
  }
}
