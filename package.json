{
  "name": "whybug-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "nodemon --watch src server.js & npm run asset-server",
    "prod": "node_modules/.bin/pm2 start server.js --name whybug -i 0",
    "stop": "node_modules/.bin/pm2 stop whybug'",
    "build": "node_modules/.bin/webpack --config config/webpack.config.js -p 2<&1",
    "asset-server": "./node_modules/.bin/webpack-dev-server --config config/webpack.config.dev.js --host 0.0.0.0 --hot --port 8001",
    "knex": "node_modules/.bin/knex --env production --cwd ./ --knexfile config/knexfile.js",
    "test": "node_modules/.bin/mocha tests --require tests/es6_mocha.js --recursive"
  },
  "dependencies": {
    "bell": "2.9.0",
    "bookshelf": "0.7.9",
    "boom": "2.9.0",
    "classnames": "2.1.3",
    "couchdb-bootstrap": "1.10.4",
    "dotenv": "1.2.0",
    "elasticsearch": "4.1.0",
    "elasticsearch-hapi-logger": "0.0.1",
    "good": "6.4.0",
    "good-console": "5.1.0",
    "good-reporter": "3.1.0",
    "graphql": "0.2.6",
    "handlebars": "2.0.0",
    "hapi": "8.8.1",
    "hapi-auth-cookie": "2.2.0",
    "hapi-cache-buster": "0.4.0",
    "hapi-relax": "1.0.1",
    "joi": "4.9.0",
    "knex": "0.6.23",
    "marked": "0.3.5",
    "mysql": "2.9.0",
    "node-uuid": "1.4.3",
    "pm2": "0.12.16",
    "pmx": "0.3.31",
    "promise": "7.0.4",
    "react": "0.13.3",
    "react-intl": "1.2.0",
    "react-markdown-textarea": "0.14.2",
    "react-router": "0.13.3",
    "slug": "0.7.1",
    "superagent": "1.4.0",
    "superagent-promise": "0.2.0"
  },
  "devDependencies": {
    "babel": "5.8.23",
    "babel-core": "5.8.25",
    "babel-loader": "5.3.2",
    "babel-runtime": "5.8.25",
    "chai": "1.10.0",
    "chai-as-promised": "4.3.0",
    "chai-react": "0.0.4",
    "css-loader": "0.9.1",
    "extract-text-webpack-plugin": "0.8.2",
    "mocha": "2.3.3",
    "node-sass": "3.3.3",
    "react-hot-loader": "1.3.0",
    "react-test-utils": "0.0.1",
    "sass-loader": "0.4.2",
    "sinon": "1.17.0",
    "sinon-chai": "2.8.0",
    "style-loader": "0.12.4",
    "supertest": "0.13.0",
    "webpack": "1.12.2",
    "webpack-dev-server": "1.12.0"
  }
}
